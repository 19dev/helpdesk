<div id="tickets_actions_list_div" class="table-responsive">
	<table class="table table-striped table-bordered table-hover" id="tickets_list_table">
	  <thead>
	  	<th class="col-md-1"><%= %></th>
	  	<th class="col-md-9"><%= t("simple_form.labels.ticket.action_desc") %></th>
	  	<th class="col-md-2"><%= t("simple_form.labels.ticket.action_date") %></th>
	  </thead>
	  <tbody>
	    <% objects.each do |ticket_action| %>
		  	<tr>
		  		<td><%= user_mini_avatar(ticket_action.user) %></td>
		  		<td><%= ticket_action.action_code %></td>
		  		<td><%= ticket_action.created_at %></td>
		  		</tr>
		  <% end %>
	  </tbody>
	  
	 </table>

	<% if objects.respond_to? :current_page %>
	  <ul class="pagination">
	   	<%= paginate objects, remote: true %>
	  </ul>
	<% end %>
</div>